<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileFragment">

    <LinearLayout
        android:background="#FFFFFFFF"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ScrollView
            android:fillViewport="true"
            android:id="@+id/rs49gus20ee"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFFFFFFF"
            android:paddingTop="55dp"
            android:clipChildren="false"
            android:clipToPadding="false">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rzow8oya5zk"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="74dp"
                    android:layout_marginHorizontal="38dp"
                    android:orientation="horizontal">
                    <RelativeLayout
                        android:id="@+id/r7zgiaeas15k_rl"
                        android:layout_width="125dp"
                        android:layout_height="125dp"
                        android:layout_marginEnd="17dp">
                        <ImageView
                            android:id="@+id/imageProfile"
                            android:layout_width="125dp"
                            android:layout_height="125dp"
                            android:background="@drawable/foto_profile"
                            android:scaleType="fitXY"
                            />

                    </RelativeLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginTop="31dp"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/haiTextView"
                                android:textColor="#000000"
                                android:textSize="20sp"
                                android:layout_marginBottom="15dp"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:text="Hai "
                                />
                            <TextView
                                android:id="@+id/nameTextView"
                                android:textColor="#000000"
                                android:textSize="20sp"
                                android:layout_marginBottom="15dp"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:text="User"
                                />
                        </LinearLayout>

                        <Button
                            android:id="@+id/editButton"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginRight="50dp"
                            android:backgroundTint="@drawable/button"
                            android:clickable="true"
                            android:focusable="true"
                            android:gravity="center_horizontal"
                            android:paddingVertical="10dp"
                            android:text="Edit"
                            android:textColor="#FFFFFF"
                            android:textSize="12sp"
                            android:textStyle="bold"/>

                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:id="@+id/emailTextView"
                    android:textColor="#000000"
                    android:textSize="14sp"
                    android:layout_marginBottom="11dp"
                    android:layout_marginHorizontal="51dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="E-mail"
                    />
                <View
                    android:id="@+id/line1"
                    android:layout_width="209dp"
                    android:layout_height="2dp"
                    android:layout_marginBottom="42dp"
                    android:layout_marginHorizontal="32dp"
                    android:background="#FF000000" />
                <TextView
                    android:id="@+id/usiaTextView"
                    android:textColor="#000000"
                    android:textSize="14sp"
                    android:layout_marginBottom="2dp"
                    android:layout_marginHorizontal="51dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Usia"
                    />
                <View
                    android:id="@+id/line2"
                    android:layout_width="209dp"
                    android:layout_height="2dp"
                    android:layout_marginBottom="42dp"
                    android:layout_marginHorizontal="32dp"
                    android:background="#FF000000" />
                <TextView
                    android:id="@+id/jkTextView"
                    android:textColor="#000000"
                    android:textSize="14sp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginHorizontal="50dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Jenis Kelamin"
                    />
                <View
                    android:id="@+id/line3"
                    android:layout_width="209dp"
                    android:layout_height="2dp"
                    android:layout_marginBottom="42dp"
                    android:layout_marginHorizontal="32dp"
                    android:background="#FF000000" />
                <TextView
                    android:id="@+id/bioTextView"
                    android:textColor="#000000"
                    android:textSize="14sp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginHorizontal="51dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Bio"
                    />
                <View
                    android:id="@+id/line4"
                    android:layout_width="209dp"
                    android:layout_height="2dp"
                    android:layout_marginBottom="42dp"
                    android:layout_marginHorizontal="32dp"
                    android:background="#FF000000" />
                <Button
                    android:id="@+id/logoutButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    android:backgroundTint="@drawable/button_simpan"
                    android:paddingVertical="12dp"
                    android:layout_marginBottom="50dp"
                    android:layout_marginHorizontal="105dp"
                    android:text="Logout" />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</FrameLayout>